#include<stdio.h>
int main() {
	int r,c,arr[10][10],i,j,sum=0;
	scanf("%d%d",&r,&c);

	for(i=0; i<r; i++)
		for(j=0; j<c; j++)
			scanf("%d",&arr[i][j]);
			
    for(j=0;j<c;j++){
        int sum=0;
        for(i=0;i<r;i++){
            sum+=arr[i][j];
        printf("%d\n",sum);
        }  
    } 
}

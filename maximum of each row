#include<stdio.h>
int main() {
	int r,c,arr[10][10],i,j,max;
	scanf("%d%d",&r,&c);

	for(i=0; i<r; i++)
		for(j=0; j<c; j++)
			scanf("%d",&arr[i][j]);
			
	for(i=0;i<r;i++){
	    int max=arr[i][0];
        for(j=0;j<c;j++)
            if(arr[i][j]>max)
            max=arr[i][j];
    printf("%d\n",max);
	}
}
